  <p>Drag and drop</p>
</div>

<div class="fixed-right">
  <span class="ship horizontal position-a" id="patrol-boat"></span>
  <span class="ship horizontal position-b" id="destroyer"></span>
  <span class="ship horizontal position-c" id="submarine"></span>
  <span class="ship horizontal position-d" id="battleship"></span>
  <span class="ship horizontal position-e" id="aircraft-carrier"></span>
  <div class="later">

    <%= form_for(@game) do |f| %>
      <% if @game.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@group.errors.count, "error") %>
            prohibited this game from being saved:</h2>
          <ul>
          <% @game.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
          </ul>
        </div>
      <% end %>

      <p>Select AI difficulty here. Large fancy radio boxes.</p>
      <%= f.submit "Board is Ready!" %>
    <% end %>

  </div>
</div>

<script>$(document).ready(boardReady);</script>
